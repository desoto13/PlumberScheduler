<%= form_with model: plumbing_request do |form| %>
    <div>
      <%= form.label :name %><br>
      <%= form.text_field :name %>
      <% plumber.errors.full_messages_for(:name).each do |message| %>
        <div><%= message %></div>
      <% end %>
    </div>

    <div>
      <%= form.label :address %><br>
      <%= form.text_area :address %><br>
      <% plumber.errors.full_messages_for(:address).each do |message| %>
        <div><%= message %></div>
      <% end %>
    </div>

    <div>
      <%= form.label :vehicle %><br>
      <%= form.text_field :vehicle %><br>
      <% plumber.errors.full_messages_for(:vehicle).each do |message| %>
        <div><%= message %></div>
      <% end %>
    </div>

  
    <div>
      <%= form.submit %>
    </div>
<% end %>